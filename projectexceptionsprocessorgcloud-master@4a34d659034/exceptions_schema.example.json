{
    "version": "1.0.0",
    "exceptions": [
        {
            "id": "100",
            "type": "create_service_accounts",
            "project_id_regex": "^prj-(rad|dev)-\\d{2}-fanta-.{3}$",
            "description": "Lab environment service accounts",
            "spec": {
                "service_accounts": [
                    {
                        "name_suffix": "api",
                        "description": "API service account",
                        "create_json_key": true,
                        "iam_roles": [
                            "roles/viewer"
                        ]
                    }
                ]
            }
        },
        {
            "id": "200",
            "type": "override_iam_policies",
            "project_id_regex": "^prj-(rad|dev)-\\d{2}-fanta-.{3}$",
            "description": "Enable service account key creation for lab environments",
            "spec": {
                "boolean_policy_overrides": [
                    {
                        "policy_name": "iam.disableServiceAccountKeyCreation",
                        "enforced": false,
                        "description": "Enable service account key creation for legacy applications"
                    }
                ]
            }
        }
    ]
}